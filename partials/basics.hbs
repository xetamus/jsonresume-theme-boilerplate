{{#resume.basics}}
        <header>
            <h1>{{name}}</h1>
            <div class="about">
                <table>
                    <tr>
                        <td><i class="fa fa-envelope-o"></i></td> 
                        <td>{{email}}</td>
                    </tr>
                    <tr>
                        <td><i class="fa fa-phone"></i></td>
                        <td>{{phone}}</td>
                    </tr>
                    <tr>
                        <td><i class="fa fa-map-pin"></i></td>
                        <td>{{location.city}}, {{location.region}}</td>
                    </tr>
                    {{#profiles}}
                    <tr>
                        <td><a href="{{url}}"><i class="fa fa-{{network}}"></i></a></td>
                        <td><a href="{{url}}">{{ url }}</a></td>
                    </tr>
                    {{/profiles}}
                </table>
            </div>
        </header>

        <div id="resume">
            <table class="section" id="blurb">
                <tr>
                    <td><i class= "fa fa-body fa-user-circle left"></i></td>
                    <td>{{summary}}</td>
                </tr>
            </table>

